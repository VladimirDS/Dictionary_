//Dictionary.cpp: Работа со словарем и строками

#include "stdafx.h"
#include <iostream>
#include <string>
#include <vector>
#include <map>

using namespace std;

struct Person {
	string name;
	string surname;
	int age;
};

int main() {
	vector<Person> staff;
	staff.push_back({ "Ivan", "Ivanov", 25 });
	staff.push_back({ "Piotr", "Gukov", 26 });

	cout << staff[1].name;

	getchar();
	return 0;
}
